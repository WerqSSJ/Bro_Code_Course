#include <iostream>
#include <ctime>


int main()
{
   srand(time(0));
   int randNum = rand()% 5 + 1;
  
  switch(randNum){
      case 1 : std::cout<<"you got the worst roll possible";
               break;
      case 2 : std::cout<<"you almost got the worst roll possible";
               break;
      case 3 : std::cout<<"you are in the midle ";
               break;
      case 4 : std::cout<<"you almost got the best roll possible";
               break;
      case 5 : std::cout<<"you got the best roll possible";
  }

    return 0;
}
